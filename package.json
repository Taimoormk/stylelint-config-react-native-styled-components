{
  "name": "stylelint-config-react-native-styled-components",
  "version": "0.1.0",
  "main": "index.js",
  "files": [
    "index.js",
    "CHANGELOG.md",
    "README.md"
  ],
  "keywords": [
    "stylelint",
    "stylelint-config",
    "react-native",
    "styled-components"
  ],
  "author": "Krister Kari",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint .",
    "release": "npmpub",
    "test": "jest"
  },
  "devDependencies": {
    "eslint": "^5.15.3",
    "eslint-plugin-jest": "^22.4.1",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "npmpub": "^4.1.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "stylelint": "^9.10.1",
    "stylelint-react-native": "^2.0.2"
  },
  "peerDependencies": {
    "stylelint": "^9.5.0",
    "stylelint-react-native": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "jest"
    ],
    "env": {
      "jest/globals": true
    },
    "extends": [
      "plugin:jest/recommended"
    ]
  }
}
